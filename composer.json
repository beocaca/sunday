{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": {
        "insane-treasurer": {
            "url": "packages/insane/treasurer",
            "type": "path",
            "options": {
                "symlink": true
            }
        }
    },
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-mailparse": "*",
        "beyondcode/laravel-websockets": "^1.9",
        "doctrine/dbal": "^2.12",
        "fideloper/proxy": "^4.2",
        "freesgen/querify": "dev-master",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "2.7",
        "google/cloud-pubsub": "^1.29",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.3.5",
        "insane/treasurer": "1.x-dev",
        "laravel/cashier": "^12.6",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^2.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "php-mime-mail-parser/php-mime-mail-parser": "^6.0",
        "pusher/pusher-php-server": "~3.0",
        "rlanvin/php-rrule": "^2.2",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.11",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "Gmail",
            "Calendar",
            "Sheets"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": "Google_Task_Composer::cleanup"
    }
}
